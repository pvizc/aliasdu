{
  "manifest_version": 3,
  "name": "aliasdu",
  "version": "0.1.0",
  "permissions": ["storage"],
  "host_permissions": ["https://api.migadu.com/*"],
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    },
    "default_title": "aliasdu"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
